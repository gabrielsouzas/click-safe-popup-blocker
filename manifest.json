{
  "manifest_version": 3,
  "name": "Click Safe",
  "version": "1.0",
  "description": "Impedir que p√°ginas sejam abertas sem seu consentimento.",
  "permissions": [
    "tabs", 
    "webNavigation",
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
